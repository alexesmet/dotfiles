/* sonokai sushia */
* {
    background:     #1a181a; /* black */
    background-mai: #2d2a2e; /* bg default */
    background-alt: #49464e; /* hl / bg4 */
    foreground:     #e3e1e4; /* fg */
    prompt:         #78dce8; /* blue */
    selected:       #e3e1e4; /* fg / yellow */
    active:         #a9dc76; /* green */
    urgent:         #ff6188; /* red */

    border-color:   #848089; /* grey */
}
